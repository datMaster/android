        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_gravity="right|top">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="#e6ff3a">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:orientation="vertical"
                    android:baselineAligned="true">

                    <EditText
                        android:layout_width="200dp"
                        android:layout_height="25dp"
                        android:inputType="none|numberDecimal"
                        android:ems="10"
                        android:id="@+id/editText"
                        android:background="@drawable/black"
                        android:clickable="false"
                        android:text="Струм : 0.0 А"
                        android:textColor="@drawable/green"
                        android:textStyle="bold"
                        android:textSize="15dp"
                        android:singleLine="true" />

                    <EditText
                        android:layout_width="200dp"
                        android:layout_height="25dp"
                        android:inputType="none|numberDecimal"
                        android:ems="10"
                        android:id="@+id/editText2"
                        android:background="@drawable/black"
                        android:textSize="15dp"
                        android:textColor="@drawable/green"
                        android:text="Напруга : 0.0 В"
                        android:textStyle="bold"
                        android:clickable="false"
                        android:singleLine="true" />

                    <Button
                        style="?android:attr/buttonStyleSmall"
                        android:layout_width="fill_parent"
                        android:layout_height="25dp"
                        android:text="Пуск"
                        android:id="@+id/button"
                        android:background="#8fff2b"
                        android:textStyle="bold"
                        android:textSize="15dp"
                        android:textColor="#000000" />

                    <Button
                        style="?android:attr/buttonStyleSmall"
                        android:layout_width="fill_parent"
                        android:layout_height="25dp"
                        android:text="Стоп"
                        android:id="@+id/button2"
                        android:background="#ff842c"
                        android:textStyle="bold"
                        android:textSize="15dp"
                        android:textColor="#0b6f00" />

                    <EditText
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:inputType="text|textMultiLine"
                        android:ems="10"
                        android:id="@+id/editText3"
                        android:gravity="top"
                        android:textSize="10dp"
                        android:textColor="#fff613"
                        android:textStyle="normal"
                        android:background="@drawable/black" />

                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

</FrameLayout>
